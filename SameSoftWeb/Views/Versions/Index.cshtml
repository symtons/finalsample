@model IEnumerable<SameSoftWeb.Models.tblAppVersion>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>App Version List</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-bordered">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.App_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Latest_Version)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Last_Update_Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Notes)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.App_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Latest_Version)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Last_Update_Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Notes)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.App_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.App_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.App_ID })
        </td>
    </tr>
}

</table>
